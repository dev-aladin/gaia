{
  "name": "gaia-hub",
  "version": "2.5.3",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@azure/storage-blob": "^10.3.0",
    "@google-cloud/storage": "^2.4.2",
    "@types/bn.js": "^4.11.5",
    "aladinjs": "^1.0.3",
    "aws-sdk": "^2.413.0",
    "bitcoinjs-lib": "^4.0.3",
    "bn.js": "^5.0.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-winston": "^3.1.0",
    "fs-extra": "^7.0.1",
    "jsontokens": "^1.0.0",
    "lru-cache": "^5.1.1",
    "node-fetch": "^2.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bitcoinjs-lib": "^4.0.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/express-winston": "^3.0.1",
    "@types/fetch-mock": "^7.2.3",
    "@types/fs-extra": "^5.0.5",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^10.12.30",
    "@types/node-fetch": "^2.1.6",
    "@types/proxyquire": "^1.3.28",
    "@types/supertest": "^2.0.7",
    "@types/tape": "^4.2.33",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.15.1",
    "fetch-mock": "^7.3.1",
    "istanbul": "^0.4.5",
    "nyc": "^13.3.0",
    "proxyquire": "^2.1.0",
    "source-map-support": "^0.5.10",
    "supertest": "^3.4.2",
    "tape": "^4.10.1",
    "tape-promise": "^4.0.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333",
    "typescript-json-schema": "^0.37.0"
  },
  "bin": {
    "aladin-gaia-hub": "./lib/index.js"
  },
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "dev": "ts-node src/index.ts",
    "build": "tsc && chmod +x lib/index.js && npm run build-schema",
    "build-schema": "typescript-json-schema tsconfig.json HubConfig --required --refs=false -o config-schema.json",
    "lint": "eslint --ext .ts ./src",
    "test": "npm run build && npm run lint && NODE_ENV=test nyc node ./test/src/index.ts"
  },
  "nyc": {
    "cache": false,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  }
}
